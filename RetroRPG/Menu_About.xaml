<Window x:Class="RetroRPG.Menu_About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RetroRPG"
        mc:Ignorable="d"
        Title="Menu_About" Height="450" Width="800">
 
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="550"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <WrapPanel Grid.Column="0" Grid.RowSpan="2" Background="Black" Name="StackMap"
                       ></WrapPanel>
            <StackPanel Grid.Column="1" Background="Black" Name="StackTiles"
                        >
                <Button Name="ExportMap" Height="30" Click="ExportMap_Click">Export</Button>
            </StackPanel>

        </Grid>

</Window>
